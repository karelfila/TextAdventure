            <Font size="53.0" />
         </font>
      </Button>
      <Button fx:id="endBtn" layoutX="163.0" layoutY="216.0" mnemonicParsing="false" onAction="#onClickEnd" prefHeight="116.0" prefWidth="327.0" stylesheets="@styles/stylesheet.css" text="Exit ">
